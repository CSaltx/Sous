// Class Usage: Initializing a class using Dish and creating a new Object

Dish Cake {
    ingredient flour;
    ingredient sugar;
    ingredient eggs;

    recipe MixIngredients() {
        serve("Mixing ingredients: ", flour, ", ", sugar, ", ", eggs);
    }

    recipe Bake() {
        serve("Baking the cake...");
    }
}

Cake myCake := new Cake("2 cups", "1 cup", "3");

myCake.MixIngredients();
myCake.Bake();